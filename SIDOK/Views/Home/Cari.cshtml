@{
    ViewData["Title"] = "Create";
}
@{
    IList<SIDOK.Models.Spesialisasi> spesialisasi = ViewData["spesialisasi"] as IList<SIDOK.Models.Spesialisasi>;
    IList<SIDOK.Models.Poli> polis = ViewData["poli"] as IList<SIDOK.Models.Poli>;
}

<div class="form-group my-2">
    <label >Spelsialisasi</label>
    <select  class="form-select" required>
        @foreach (var spesisalis in spesialisasi)
        {
            <option value="@spesisalis.Id">@spesisalis.Nama</option>
        }
    </select>
</div>
<div class="form-group my-2">
    <label>Poli</label>
    <select class="form-select" required>
        @foreach (var poli in polis)
        {
            <option value="@poli.IdPoli">@poli.NamaPoli</option>
        }
    </select>
</div>